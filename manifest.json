{
  "name": "tumblr-power-pak",
  "version": "1.0.0",
  "description": "Fix some of the most annoying Tumblr quirks.",
  "background": {
    "page":"bg.html"
  },
  "manifest_version": 2,
  "permissions": ["tabs", "webNavigation"],
  "content_scripts": [
    {
      "matches": ["http://*.tumblr.com/archive*"],
      "js": ["archive.js"]
    },
    {
      "matches": ["*://*/image/*"],
      "js": ["image.js"]
    }
  ]
}
